<?xml version="1.0" encoding="UTF-8"?>
<web-app version="4.0" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd">
    <servlet>
        <servlet-name>MiServlet</servlet-name>
        <servlet-class>MiServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Ejercicio4</servlet-name>
        <servlet-class>Ejercicio4</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RespuestaServlet</servlet-name>
        <servlet-class>RespuestaServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>MiServlet</servlet-name>
        <url-pattern>/MiServlet</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>inicio.html</welcome-file>
    </welcome-file-list>
    <servlet-mapping>
        <servlet-name>Ejercicio4</servlet-name>
        <url-pattern>/Ejercicio4</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RespuestaServlet</servlet-name>
        <url-pattern>/respuesta</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
